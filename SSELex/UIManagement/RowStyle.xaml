<Window x:Class="SSELex.UIManagement.RowStyle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SSELex.UIManagement"
        mc:Ignorable="d"
        Title="RowStyle" Height="450" Width="800" Background="{x:Null}">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="0 20 0 20">
            <!--42 DefLineHeight-->
            <Grid Height="42" Background="#FF232323">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="5"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="0" Background="Black">
                </Grid>
            </Grid>
            <!--82 Height-->
        </StackPanel>
    </Grid>
</Window>
